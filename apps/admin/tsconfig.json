{
  "extends": "../../tsconfig.base.json",
  "include": ["src"],
  "compilerOptions": {
    "outDir": "dist",
    "lib": ["ES2022", "DOM"],
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "noImplicitAny": false,
    "strict": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}



